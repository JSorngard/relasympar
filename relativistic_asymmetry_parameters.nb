           RowBox[{"(*", 
            RowBox[{"Sum", " ", "incoherently", " ", "over", " ", "the", " ", 
             SubscriptBox["m", "j"], " ", "quantum", " ", "number", " ", 
             "and", " ", "coherently", " ", "over", " ", "the", " ", "K", " ",
              "and", " ", "\[Kappa]", " ", "values", " ", "of", " ", "the", 
             " ", "final", " ", "state"}], "*)"}], 
           RowBox[{"Sum", "[", 
            RowBox[{
             RowBox[{"Quiet", "@", 
              RowBox[{"Sum", "[", "\[IndentingNewLine]", 
               RowBox[{"(*", 
                RowBox[{
                "Compute", " ", "the", " ", "l", " ", "and", " ", "j", " ", 
                 "values", " ", "of", " ", "the", " ", "upper", " ", "and", 
                 " ", "lower", " ", "components", " ", "in", " ", "the", " ", 
                 "final", " ", 
                 RowBox[{"state", ".", " ", "Each"}], " ", "of", " ", "these",
                  " ", "come", " ", "in", " ", "two", " ", "copies", " ", 
                 "of", " ", "themselves", " ", "to", " ", "enable", " ", 
                 "coherent", " ", 
                 RowBox[{"summation", "."}]}], "*)"}], "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{"{", 
                   RowBox[{"lf", ",", "j"}], "}"}], "=", 
                  RowBox[{"lj", "[", "\[Kappa]", "]"}]}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"{", 
                   RowBox[{"lfp", ",", "jp"}], "}"}], "=", 
                  RowBox[{"lj", "[", "\[Kappa]p", "]"}]}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"lg", "=", 
                  RowBox[{"lf", "-", 
                   RowBox[{"Sign", "[", "\[Kappa]", "]"}]}]}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"lgp", "=", 
                  RowBox[{"lfp", "-", 
                   RowBox[{"Sign", "[", "\[Kappa]p", "]"}]}]}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{
                  SuperscriptBox[
                   RowBox[{"(", 
                    RowBox[{"-", "1"}], ")"}], 
                   RowBox[{"j", "+", "k", "+", "j0"}]], 
                  SuperscriptBox[
                   RowBox[{"(", 
                    RowBox[{"-", "1"}], ")"}], 
                   RowBox[{"j", "-", "mj"}]], 
                  SuperscriptBox[
                   RowBox[{"(", 
                    RowBox[{"-", "1"}], ")"}], 
