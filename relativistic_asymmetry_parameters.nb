                FractionBox[
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"2", 
                    SubscriptBox["l", "f"]}], "+", "1"}], ")"}], 
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"2", 
                    SubscriptBox[
                    RowBox[{"l", "'"}], "f"]}], "+", "1"}], ")"}], 
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"2", "l"}], "+", "1"}], ")"}]}], 
                 RowBox[{"4", "\[Pi]"}]]], 
               RowBox[{
