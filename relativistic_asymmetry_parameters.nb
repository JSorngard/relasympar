                    RowBox[{"{", 
                    RowBox[{"k", ",", "0"}], "}"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"j0", ",", "mj"}], "}"}]}], "]"}], 
                  RowBox[{"ThreeJSymbol", "[", 
                   RowBox[{
                    RowBox[{"{", 
                    RowBox[{"jp", ",", 
                    RowBox[{"-", "mj"}]}], "}"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"kp", ",", "0"}], "}"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"j0", ",", "mj"}], "}"}]}], "]"}], 
                  RowBox[{"(", 
                   RowBox[{"Sum", "[", 
                    RowBox[{"(*", 
                    RowBox[{
